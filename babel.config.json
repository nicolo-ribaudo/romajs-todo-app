{
	"presets": [
		["@babel/preset-env", {
			"shippedProposals": true
		}],
		["@babel/preset-react", {
			"runtime": "automatic",
			"importSource": "preact"
		}]
	],
	"plugins": [
		["polyfill-corejs3", { "method": "usage-global" }],
		["@babel/plugin-transform-runtime", {
			"version": "7.13.10"
		}]
	]
}
